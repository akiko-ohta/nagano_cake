<table class="table table-bordered">
  <th class="text-center">ジャンル検索</th>
    <% genres.each do |genre| %>
    <tr>
      <td>
          <%= link_to items_path(genre_name: genre.name), class: 'text-decoration-none text-dark  btn-default' do %>
            <%= genre.name %>
          <% end %>
      </td>
    </tr>
    <% end %>
</table>